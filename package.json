{
  "name": "@motss/signatur",
  "version": "0.0.3",
  "description": "Sign and unsign HTTP request with ease",
  "keywords": [
    "http-signature",
    "node",
    "sign-http-request",
    "signatur",
    "signature",
    "signed-request",
    "typescript",
    "unsign-http-request"
  ],
  "homepage": "https://github.com/motss/signatur",
  "repository": {
    "type": "git",
    "url": "git@github.com:motss/signatur.git"
  },
  "license": "MIT",
  "author": {
    "name": "Rong Sen Ng",
    "email": "wes.ngrongsen@gmail.com",
    "url": "https://github.com/motss"
  },
  "files": [
    "index.js",
    "index.mjs",
    "index.d.ts"
  ],
  "main": "./index",
  "module": "./index.mjs",
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm && rm -rf dist/ && echo \"> [build] Done!\"",
    "build:cjs": "npm run lint && rm -rf dist/ && tsc -p tsconfig.json && mv dist/index.js dist/index.d.ts . && echo \"> [cjs] Done!\"",
    "build:esm": "npm run lint && rm -rf dist/ && tsc -p tsconfig.esm.json && mv dist/index.js ./index.mjs && echo \"> [esm] Done!\"",
    "demo": "node demo/index.js",
    "lint": "tslint --project tsconfig.json --config tslint.prod.json --format stylish 'src/**/*.ts*'",
    "lint:debug": "tslint --project tsconfig.json --config tslint.json --format stylish 'src/**/*.ts*'",
    "prepublishOnly": "npm run lint && npm run build",
    "test": "jest --detectOpenHandles --config ./.jestrc.json"
  },
  "dependencies": {},
  "devDependencies": {
    "@messageflow/tslint-config": "^1.1.0",
    "@types/jest": "^23.3.1",
    "@types/node": "^10.9.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "jest": "^23.5.0",
    "ts-jest": "^23.1.4",
    "tslint": "^5.11.0",
    "typescript": "^3.0.3"
  },
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 5.5.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
